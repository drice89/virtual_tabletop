{
  "name": "virtual_tabletop",
  "version": "1.0.0",
  "description": "open source application provide a sandbox tabletop enviornment ",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "server:debug": "nodemon --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drice89/virtual_tabletop.git"
  },
  "author": "Dias Iskrayev, Dillon Rice, Steven Touba, Wayne Su",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/drice89/virtual_tabletop/issues"
  },
  "homepage": "https://github.com/drice89/virtual_tabletop#readme",
  "dependencies": {
    "aws-sdk": "^2.676.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "concurrently": "^5.2.0",
    "express": "^4.18.2",
    "form-data": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.13",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-router-dom": "^5.2.0",
    "socket.io": "^2.3.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "nodemon": "^2.0.3"
  }
}
